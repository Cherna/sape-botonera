{
  "name": "botonera",
  "version": "0.0.0",
  "description": "Plain webpack 4 Boilerplate with Babel, SASS and lodash on board",
  "author": "",
  "license": "MIT",
  "homepage": "",
  "scripts": {
    "start": "node ./tools && npm run dev",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --mode development --config webpack.config.dev.js",
    "build": "webpack -p --progress --mode production --config webpack.config.build.js"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "lodash": "4.17.10"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "8.0.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-env": "1.7.0",
    "clean-webpack-plugin": "0.1.19",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "4.9.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "url-loader": "1.1.1",
    "webpack": "4.17.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.7",
    "webpack-merge": "4.1.4",
    "webpack-shell-plugin": "^0.5.0"
  }
}
